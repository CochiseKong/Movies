{
  "name": "Movies",
  "version": "1.0.0",
  "description": "预告片网站",
  "main": "start.js",
  "scripts": {
    "precommit": "standard --parser src/**/*.js server/**/*.js | snazzy",
    "start": "rm -rf dist && rm -rf .cache && NODE_ENV=development nodemon ./start.js",
    "prod": "yarn build && rm -rf .cache && NODE_ENV=production nodemon ./start.js",
    "build": "rm -rf dist && parcel build src/index.html --no-cache -d dist/ --public-url /"
  },
